<h1>Listing tccapis</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tccapis.each do |tccapi| %>
      <tr>
        <td><%= link_to 'Show', tccapi %></td>
        <td><%= link_to 'Edit', edit_tccapi_path(tccapi) %></td>
        <td><%= link_to 'Destroy', tccapi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tccapi', new_tccapi_path %>
